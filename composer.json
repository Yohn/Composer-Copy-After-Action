{
  "name": "yohns/composer-copy-after-action",
  "description": "testing copying",
  "license": "MIT",
  "authors": [
		{
			"name": "Yohn",
			"email": "john.skem9@gmail.com"
		}
  ],
  "require": {
		"markocupic/composer-file-copier-plugin": "dev-main"
	},
	"config": {
		"allow-plugins": {
			"markocupic/composer-file-copier-plugin": true
		}
	},
	"extra": {
		"composer-file-copier-plugin": [
			{
				"source": "testing-copy.php",
				"target": "lib/Config/testing-copy.php",
				"options": {
					"OVERRIDE": true
				}
			}
		]
	}
}
